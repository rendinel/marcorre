{
  "name": "@chakra-ui/pro-theme",
  "version": "0.0.47",
  "license": "UNLICENSED",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "vite build && pnpm types",
    "bundle": "pro-scripts archive --name chakra-ui-pro-theme",
    "release": "release-it --ci",
    "types": "tsc src/index.ts  --skipLibCheck --emitDeclarationOnly --declaration --declarationMap --declarationDir dist"
  },
  "dependencies": {
    "@chakra-ui/theme-tools": "1.3.6",
    "focus-visible": "5.2.0"
  },
  "devDependencies": {
    "@chakra-ui-pro/scripts": "*",
    "@chakra-ui/react": "1.8.8",
    "release-it": "14.13.1",
    "rimraf": "3.0.2",
    "typescript": "4.6.3",
    "vite": "2.8.6"
  },
  "peerDependencies": {
    "@chakra-ui/react": "^1.8.0"
  }
}
